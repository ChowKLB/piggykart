<%= content_for :return_to_path, return_to_path(params, imports_path) %>

<%= content_for :nav do %>
  <%= render "imports/nav", import: @import %>
<% end %>

<div>
  <h1 class="sr-only">Clean import</h1>

  <div>
    <%= render partial: "imports/rows/row", collection: @import.rows %>
  </div>

  <%= link_to "Next", confirm_import_path(@import), class: "px-4 py-2 block w-full rounded-lg bg-gray-900 text-white text-sm font-medium", data: { turbo: false } %>
</div>
